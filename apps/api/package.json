{
  "name": "markitos",
  "version": "1.0.0",
  "description": "markitos ts api",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/start.ts",
    "start": "npm run build && node ./dist/src/start.js",
    "test": "NODE_ENV=dev jest --runInBand --detectOpenHandles --forceExit --watchAll",
    "citest": "jest --forceExit  --passWithNoTests --coverage",
    "clean": "rm -r ./dist; exit 0",
    "lint": "eslint --fix . --ext .ts,.tsx ./src",
    "format": "prettier --write ./src ./test"
  },
  "author": "Marco Antonio <marcokitos.info@gmail.com>",
  "license": "ISC",
